<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UITabForm>
    <label>
      <showConnectCloudDrive>Connect Cloud Documents</showConnectCloudDrive>
      <connectGoogleDrive>Connect Google Drive</connectGoogleDrive>
      <connectBox>Connect Box</connectBox>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Connect Cloud Documents</ConnectCloudDriveForm>
      <ConnectYour>Connect your</ConnectYour>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Connect Cloud Documents</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <UIActionBar>
    <tooltip>
      <ConnectGoogleDrive>Connect your Google Drive</ConnectGoogleDrive>
    </tooltip>
  </UIActionBar>
  <UIActionBar>
    <tooltip>
      <ConnectBox>Connect your Box</ConnectBox>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Open on</OpenCloudFile>
    </event>
    <event>
      <RefreshCloudDrive>Sync with </RefreshCloudDrive>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Open $fileUrl on $providerName</OpenFileOn>
    </event>
    <event>
      <OpenOn>Open on</OpenOn>
    </event>
    <event>
      <EditFile>Edit</EditFile>
    </event>
    <event>
      <ViewFile>View</ViewFile>
    </event>
  </CloudFileViewer>
  <CloudDriveClipboard>
    <msg>
      <CloudFilePasteToAnotherDriveNotSupported>Cannot paste cloud file to another cloud drive</CloudFilePasteToAnotherDriveNotSupported>
    </msg>
    <msg>
      <CloudFilePasteMoveToOutsideNotSupported>Cannot cut-paste (move) cloud file to outside its cloud drive</CloudFilePasteMoveToOutsideNotSupported>
    </msg>
    <msg>
      <CloudFilePasteBetweenWorkspacesNotSupported>Cannot paste (move) cloud file from different workspace</CloudFilePasteBetweenWorkspacesNotSupported>
    </msg>
  </CloudDriveClipboard>
</bundle>