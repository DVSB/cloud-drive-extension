<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UITabForm>
    <label>
      <showConnectCloudDrive>Connect Cloud Documents</showConnectCloudDrive>
      <connectGoogleDrive>Connect Google Drive</connectGoogleDrive>
      <connectBox>Connect Box</connectBox>
    </label>
  </UITabForm>
  <UIPopupWindow>
    <title>
      <ConnectCloudDriveForm>Connect Cloud Documents</ConnectCloudDriveForm>
      <ConnectYour>Connect your</ConnectYour>
    </title>
  </UIPopupWindow>
  <UIActionBar>
    <tooltip>
      <ShowConnectCloudDrive>Connect Cloud Documents</ShowConnectCloudDrive>
    </tooltip>
  </UIActionBar>
  <UIActionBar>
    <tooltip>
      <ConnectGoogleDrive>Connect your Google Drive</ConnectGoogleDrive>
    </tooltip>
  </UIActionBar>
  <UIActionBar>
    <tooltip>
      <ConnectBox>Connect your Box</ConnectBox>
    </tooltip>
  </UIActionBar>
  <ECMContextMenu>
    <event>
      <OpenCloudFile>Open on</OpenCloudFile>
    </event>
    <event>
      <RefreshCloudDrive>Sync with </RefreshCloudDrive>
    </event>
  </ECMContextMenu>
  <CloudFileViewer>
    <event>
      <OpenFileOn>Open $fileUrl on $providerName</OpenFileOn>
    </event>
    <event>
      <OpenOn>Open on</OpenOn>
    </event>
    <event>
      <EditFile>Edit</EditFile>
    </event>
    <event>
      <ViewFile>View</ViewFile>
    </event>
  </CloudFileViewer>
  <CloudFile>
    <msg>
      <MoveToAnotherDriveNotSupported>Cannot move file to another cloud drive</MoveToAnotherDriveNotSupported>
    </msg>
    <msg>
      <MoveToOutsideDriveNotSupported>Cannot move file to outside its cloud drive</MoveToOutsideDriveNotSupported>
    </msg>
    <msg>
      <MoveBetweenWorkspacesNotSupported>Cannot move cloud file to different workspace</MoveBetweenWorkspacesNotSupported>
    </msg>
    <msg>
      <LinkCreated>Link{0} created</LinkCreated>
    </msg>
    <msg>
      <FileLinksCreated>File{0} linked in {1}</FileLinksCreated>
    </msg>
  </CloudFile>
</bundle>